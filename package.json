{
  "name": "ni-admin-vue",
  "type": "module",
  "version": "0.0.1",
  "author": "rogepi",
  "description": "a simple admin template with vue & naive ui",
  "private": false,
  "packageManager": "pnpm@7.4.0",
  "scripts": {
    "build": "vite build",
    "dev": "vite --port 3333 --open",
    "lint": "eslint .",
    "typecheck": "vue-tsc --noEmit",
    "preview": "vite preview",
    "test": "vitest",
    "up": "taze major -I"
  },
  "dependencies": {
    "@vueuse/core": "^9.2.0",
    "vue": "^3.2.39",
    "vue-router": "^4.1.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.27.0",
    "@iconify-json/carbon": "^1.1.8",
    "@types/node": "^18.7.18",
    "@unocss/reset": "^0.45.22",
    "@vitejs/plugin-vue": "^3.1.0",
    "@vue/test-utils": "^2.0.2",
    "eslint": "^8.23.1",
    "jsdom": "^20.0.0",
    "lint-staged": "^13.0.3",
    "pnpm": "^7.12.1",
    "simple-git-hooks": "^2.8.0",
    "taze": "^0.8.1",
    "typescript": "^4.8.3",
    "unocss": "^0.45.22",
    "unplugin-auto-import": "^0.11.2",
    "unplugin-vue-components": "^0.22.7",
    "vite": "^3.1.3",
    "vite-plugin-pages": "^0.26.0",
    "vitest": "^0.23.4",
    "vue-tsc": "^0.40.13"
  },
  "eslintConfig": {
    "extends": "@antfu"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,tsx,vue,md}": [
      "eslint --fix"
    ]
  }
}
